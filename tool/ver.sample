package VER;

message VersionInfo {
	required float version = 2;
} // end

message VersionInfoReply {
	required Result res = 2;

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message ClientVersionInfo {
	required float version = 2;
} // end

message ClientVersionInfoReply {
	required Result res = 2;

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end
