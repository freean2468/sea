package S2C;

message RegisterAccountReply {
	required string k_id = 2;
	required Result res = 3 [default=FALSE];

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message UnregisterAccountReply {
	required string k_id = 2;
	required Result res = 3 [default = FALSE];

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message StartGameReply {
	required string k_id = 2;
	required Result res = 3 [default=FALSE];
	required int32 heart = 4 [default=0];
	required int64 last_charged_time = 5 [default=0];
	
	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message AccountInfo {
	required string k_id = 2;
	required Result res = 3 [default=FALSE];
	required int32 coin = 4 [default=0];
	required int32 mineral = 5 [default=0];
	required int32 lv = 6 [default=0];
	required int32 exp = 7 [default=0];
	required int32 point = 8 [default=0];
	required int32 heart = 9 [default=0];
	required int64 last_charged_time = 10 [default=0];
	required int32 selected_character = 11 [default=0];
	required int32 selected_assistant = 12 [default=0];
	required int32 characters = 13 [default=0];
	required int32 basic_charac_lv = 14 [default=0];
	required int32 assistants = 15 [default=0];
	required int32 basic_assist_lv = 16 [default=0];
	required int32 items = 17 [default=0];
	required int32 count = 18 [default=0];
	
	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message ChargeInfo {
	required string k_id = 2;
	required Result res = 3 [default = FALSE];
	required int32 heart = 4 [default=0];
	required int64 last_charged_time = 5 [default=0];

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message RankInfo {
	required string k_id = 2;
	required Result res = 3 [default=FALSE];
	required int64 overall_ranking = 4 [default=0];
	repeated FriendRankInfo ranking_list = 5;
	
	message FriendRankInfo {
		required string k_id = 1;
		required int32 score = 2;
	}
	
	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message GameResult {
	required string k_id = 2;
	required Result res = 3 [default = FALSE];
	required int32 score = 4 [default=0];
	
	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message VersionInfoReply {
	required Result res = 2 [default = FALSE];

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end

message ClientVersionInfoReply {
	required Result res = 2 [default = FALSE];

	enum Result {
		FALSE	= 0;
		TRUE	= 1;
	}
} // end
